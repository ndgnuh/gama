<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.decorators">
      <decorator
            adaptable="false"
            class="gama.ui.navigator.support.NavigatorBaseLighweightDecorator"
            id="gama.core.application.decorator"
            label="Metadata Decorator"
            lightweight="true"
            location="REPLACE"
            state="true">
         <enablement>
            <or>
               <objectClass
                     name="gama.ui.navigator.contents.LinkedFile">
               </objectClass>
               <objectClass
                     name="gama.ui.navigator.contents.WrappedProject">
               </objectClass>
               <objectClass
                     name="gama.ui.navigator.contents.WrappedLink">
               </objectClass>
               <and>
                  <objectClass
                        name="org.eclipse.core.resources.IFile">
                  </objectClass>
                  <or>
                     <objectState
                           name="contentTypeId"
                           value="gama.gui.gaml.type">
                     </objectState>
                     <objectState
                           name="contentTypeId"
                           value="gama.gui.shapefile.type">
                     </objectState>
                     <objectState
                           name="contentTypeId"
                           value="gama.gui.csv.type">
                     </objectState>
                     <objectState
                           name="contentTypeId"
                           value="gama.gui.images.type">
                     </objectState>
                     <objectState
                           name="contentTypeId"
                           value="gama.gui.shapefile.support.type">
                     </objectState>
                     <objectState
                           name="contentTypeId"
                           value="gama.gui.osm.type">
                     </objectState>
                     <objectState
                           name="contentTypeId"
                           value="gama.gui.gsim.type">
                     </objectState>                     
                  </or>
               </and>
               <objectClass
                     name="gama.ui.navigator.contents.WrappedFolder">
               </objectClass>
               <objectClass
                     name="gama.ui.navigator.contents.TopLevelFolder">
               </objectClass>
               <objectClass
                     name="gama.ui.navigator.contents.WrappedFile">
               </objectClass>
               <objectClass
                     name="gama.ui.navigator.contents.WrappedGamaFile">
               </objectClass>
               <objectClass
                     name="gama.ui.navigator.contents.WrappedSyntacticContent">
               </objectClass>
            </or>
         </enablement>
      </decorator>
      <decorator
            adaptable="true"
            class="gama.ui.navigator.support.NavigatorDateLighweightDecorator"
            id="gama.core.application.date.decorator"
            label="Date Decorator"
            lightweight="true"
            state="false">
      </decorator>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="true"
            locationURI="popup:navigator.menu?after=group.new">
         <menu
               icon="platform:/plugin/gama.ui.base/icons/navigator/navigator.new2.png"
               id="menu.new"
               label="New...">
            <command
                  commandId="org.eclipse.ui.newWizard"
                  icon="platform:/plugin/gama.ui.base/icons/navigator/new.project2.png"
                  label="Project..."
                  style="push">
               <parameter
                     name="newWizardId"
                     value="gama.gui.wizards.newProjectWizard">
               </parameter>
               <visibleWhen
                     checkEnabled="false">
                  <with
                        variable="selection">
                     <iterate
                           ifEmpty="true"
                           operator="and">
                        <or>
                           <instanceof
                                 value="gama.ui.navigator.contents.UserProjectsFolder">
                           </instanceof>
                        </or>
                     </iterate>
                  </with>
               </visibleWhen>
            </command>
            <command
                  commandId="org.eclipse.ui.newWizard"
                  icon="platform:/plugin/gama.ui.base/icons/navigator/new.model2.png"
                  label="Model…"
                  style="push">
               <parameter
                     name="newWizardId"
                     value="gama.gui.wizards.NewFileWizard">
               </parameter>
            </command>
            <command
                  commandId="org.eclipse.ui.newWizard"
                  icon="platform:/plugin/gama.ui.base/icons/navigator/new.model2.png"
                  label="Experiment…"
                  style="push">
               <parameter
                     name="newWizardId"
                     value="gama.gui.wizards.NewExperimentWizard">
               </parameter>
            </command>
            <command
                  commandId="org.eclipse.ui.newWizard"
                  icon="platform:/plugin/gama.ui.base/icons/navigator/new.model2.png"
                  label="Test…"
                  style="push">
               <parameter
                     name="newWizardId"
                     value="gama.gui.wizards.NewTestExperimentWizard">
               </parameter>
            </command>
            <separator
                  name="gama.core.application.separator22"
                  visible="true">
            </separator>
            <command
                  commandId="org.eclipse.ui.newWizard"
                  icon="platform:/plugin/gama.ui.base/icons/navigator/navigator.new2.png"
                  label="Other..."
                  style="push">
            </command>
            <visibleWhen
                  checkEnabled="false">
               <with
                     variable="selection">
                  <iterate
                        ifEmpty="true"
                        operator="and">
                     <or>
                        <instanceof
                              value="gama.ui.navigator.contents.WrappedProject">
                        </instanceof>
                        <instanceof
                              value="gama.ui.navigator.contents.WrappedFolder">
                        </instanceof>
                        <instanceof
                              value="gama.ui.navigator.contents.UserProjectsFolder">
                        </instanceof>
                     </or>
                  </iterate>
               </with>
            </visibleWhen>
         </menu>
         <menu
               icon="platform:/plugin/gama.ui.base/icons/navigator/navigator.import2.png"
               id="menu.gama.import"
               label="Import..."
               mnemonic=" ">
            <command
                  commandId="org.eclipse.ui.file.import"
                  icon="platform:/plugin/gama.ui.base/icons/navigator/navigator.import.project2.png"
                  label="Project…"
                  style="push">
               <parameter
                     name="importWizardId"
                     value="gama.ui.base.import.ExternalProject">
               </parameter>
               <visibleWhen
                     checkEnabled="false">
                  <with
                        variable="selection">
                     <iterate
                           ifEmpty="true"
                           operator="and">
                        <or>
                           <instanceof
                                 value="gama.ui.navigator.contents.UserProjectsFolder">
                           </instanceof>
                        </or>
                     </iterate>
                  </with>
               </visibleWhen>
            </command>
            <command
                  commandId="org.eclipse.ui.file.import"
                  icon="platform:/plugin/gama.ui.base/icons/navigator/navigator.import.disk2.png"
                  label="From disk..."
                  style="push">
               <parameter
                     name="importWizardId"
                     value="gama.ui.base.import.ExternalFile">
               </parameter>
            </command>
            <command
                  commandId="org.eclipse.ui.file.import"
                  icon="platform:/plugin/gama.ui.base/icons/navigator/navigator.import.archive2.png"
                  label="From archive..."
                  style="push">
               <parameter
                     name="importWizardId"
                     value="gama.ui.base.import.ZipFile">
               </parameter>
            </command>
            <separator
                  name="gama.core.application.separator1"
                  visible="true">
            </separator>
            <command
                  commandId="org.eclipse.ui.file.import"
                  icon="platform:/plugin/gama.ui.base/icons/navigator/navigator.import2.png"
                  label="Other..."
                  style="push">
            </command>
            <visibleWhen
                  checkEnabled="false">
               <with
                     variable="selection">
                  <iterate
                        ifEmpty="true"
                        operator="and">
                     <or>
                        <instanceof
                              value="gama.ui.navigator.contents.WrappedProject">
                        </instanceof>
                        <instanceof
                              value="gama.ui.navigator.contents.WrappedFolder">
                        </instanceof>
                        <instanceof
                              value="gama.ui.navigator.contents.UserProjectsFolder">
                        </instanceof>
                     </or>
                  </iterate>
               </with>
            </visibleWhen>
         </menu>
         <separator
               name="gama.gui.gamaNavigator.separator1"
               visible="true">
         </separator>
         <menu
               icon="platform:/plugin/gama.ui.base/icons/menu.export2.png"
               id="menu.gama.export"
               label="Export...">
            <command
                  commandId="org.eclipse.ui.file.export"
                  icon="platform:/plugin/gama.ui.base/icons/navigator/navigator.import.project2.png"
                  label="Project…"
                  style="push">
               <parameter
                     name="exportWizardId"
                     value="gama.ui.base.export.ExternalProject">
               </parameter>
            </command>
            <visibleWhen
                  checkEnabled="false">
               <with
                     variable="selection">
                  <iterate
                        ifEmpty="true"
                        operator="and">
                     <or>
                        <instanceof
                              value="gama.ui.navigator.contents.WrappedProject">
                        </instanceof>
                        <instanceof
                              value="gama.ui.navigator.contents.WrappedFolder">
                        </instanceof>
                     </or>
                  </iterate>
               </with>
            </visibleWhen>
           
         </menu>
         <separator
               name="gama.ui.navigator.separator2"
               visible="true">
         </separator>
         <command
               commandId="org.eclipse.ui.ide.showInSystemExplorer"
               icon="platform:/plugin/gama.ui.base/icons/menu.searchfile2.png"
               label="Reveal in OS"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <with
                     variable="selection">
                  <iterate
                        ifEmpty="false"
                        operator="and">
                     <adapt
                           type="org.eclipse.core.resources.IResource">
                     </adapt>
                  </iterate>
               </with>
            </visibleWhen>
         </command>
         <command
               commandId="gama.core.navigator.reveal"
               icon="platform:/plugin/gama.ui.base/icons/menu.searchfile2.png"
               label="Reveal in Workspace"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <with
                     variable="selection">
                  <iterate
                        ifEmpty="false"
                        operator="and">
                     <instanceof
                           value="gama.ui.navigator.contents.LinkedFile">
                     </instanceof>
                  </iterate>
               </with>
            </visibleWhen>
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="menu:new?after=new">
         <command
               commandId="org.eclipse.ui.newWizard"
               icon="platform:/plugin/gama.ui.base/icons/navigator/new.project2.png"
               label="GAMA Project..."
               style="push">
            <parameter
                  name="newWizardId"
                  value="gama.gui.wizards.newProjectWizard">
            </parameter>
            <visibleWhen
                  checkEnabled="false">
               <with
                     variable="selection">
                  <iterate
                        ifEmpty="true"
                        operator="and">
                     <or>
                        <instanceof
                              value="gama.ui.navigator.contents.UserProjectsFolder">
                        </instanceof>
                     </or>
                  </iterate>
               </with>
            </visibleWhen>
         </command>
         <command
               commandId="org.eclipse.ui.newWizard"
               icon="platform:/plugin/gama.ui.base/icons/navigator/new.model2.png"
               label="Model File…"
               style="push">
            <parameter
                  name="newWizardId"
                  value="gama.gui.wizards.NewFileWizard">
            </parameter>
         </command>
         <command
               commandId="org.eclipse.ui.newWizard"
               icon="platform:/plugin/gama.ui.base/icons/navigator/new.model2.png"
               label="Experiment…"
               style="push">
            <parameter
                  name="newWizardId"
                  value="gama.gui.wizards.NewExperimentWizard">
            </parameter>
         </command>
         <command
               commandId="org.eclipse.ui.newWizard"
               icon="platform:/plugin/gama.ui.base/icons/navigator/new.model2.png"
               label="Test…"
               style="push">
            <parameter
                  name="newWizardId"
                  value="gama.gui.wizards.NewTestExperimentWizard">
            </parameter>
         </command>
         <separator
               name="gama.core.application.separator10"
               visible="true">
         </separator>
         <command
               commandId="org.eclipse.ui.newWizard"
               icon="platform:/plugin/gama.ui.base/icons/navigator/navigator.new2.png"
               label="Other..."
               style="push">
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.navigator.viewer">
      <viewer
            viewerId="gama.gui.view.GamaNavigator">
         <popupMenu
                allowsPlatformContributions="true"
                id="navigator.menu">  
             <insertionPoint name="group.new"/>
             <insertionPoint
                   name="group.open"
                   separator="true"/>
             <insertionPoint name="group.openWith"/>   
             <insertionPoint name="group.copy"
                   separator="true"/>   
             <insertionPoint
                   name="group.refresh"
                   separator="true"/>
             <insertionPoint
                   name="additions"
                   separator="true">
             </insertionPoint>             
          </popupMenu>
      </viewer>
      <viewerActionBinding
            viewerId="gama.gui.view.GamaNavigator">
         <includes>
            <actionExtension
                  pattern="edit.actions">
            </actionExtension>
            <actionExtension
                  pattern="openclose.actions">
            </actionExtension>
            <actionExtension
                  pattern="refresh.actions">
            </actionExtension>
            <actionExtension
                  pattern="refactor.actions">
            </actionExtension>
            <actionExtension
                  pattern="open.actions">
            </actionExtension>
            <actionExtension
                  pattern="gaml.actions">
            </actionExtension>
         </includes>
         <excludes>
            <actionExtension
                  pattern="org.eclipse.ui.*">
            </actionExtension>
            <actionExtension
                  pattern="org.eclipse.team.ui.*">
            </actionExtension>
            <actionExtension
                  pattern="org.eclipse.ui.internal.navigator.resources.actions.*">
            </actionExtension>
            <actionExtension
                  pattern="org.eclipse.ui.navigator.resources.actions.*">
            </actionExtension>
            <actionExtension
                  pattern="org.eclipse.internal.ui.*">
            </actionExtension>
            <actionExtension
                  pattern="org.eclipse.team.internal.ui.*">
            </actionExtension>
            <actionExtension
                  pattern="org.eclipse.xtext.builder.action.*">
            </actionExtension>
         </excludes>
      </viewerActionBinding>
      <viewerContentBinding
            viewerId="gama.gui.view.GamaNavigator">
         <includes>
            <contentExtension
                  pattern="gama.core.application.shapefile.filter">
            </contentExtension>
            <contentExtension
                  isRoot="true"
                  pattern="gama.core.application.resourceVirtualNavigatorContent">
            </contentExtension>
            <contentExtension
                  pattern="gama.ui.navigator.linkHelper1">
            </contentExtension>
         </includes>
      </viewerContentBinding>
      <dragAssistant
            class="org.eclipse.ui.navigator.resources.ResourceDragAdapterAssistant"
            viewerId="gama.gui.view.GamaNavigator">
      </dragAssistant>
   </extension>
   <extension
         point="org.eclipse.ui.navigator.navigatorContent">
      <navigatorContent
            activeByDefault="true"
            appearsBefore="org.eclipse.ui.navigator.resourceContent"
            contentProvider="gama.ui.navigator.support.NavigatorContentProvider"
            id="gama.core.application.resourceVirtualNavigatorContent"
            labelProvider="gama.ui.navigator.support.NavigatorLabelProvider"
            name="Virtual GamaNavigator Content"
            priority="highest"
            providesSaveables="false">
         <triggerPoints>
            <or>
               <instanceof
                     value="gama.ui.navigator.contents.VirtualContent">
               </instanceof>

            </or>
         </triggerPoints>
         <possibleChildren>
            <or>
               <instanceof
                     value="gama.ui.navigator.contents.LinkedFile">
               </instanceof>
               <instanceof
                     value="gama.ui.navigator.contents.Category">
               </instanceof>
               <instanceof
                     value="gama.ui.navigator.contents.VirtualContent">
               </instanceof>
            </or>
         </possibleChildren>
         <commonSorter
               class="gama.ui.navigator.support.NavigatorSorter"
               id="gama.navigator.sorter">
            <parentExpression>
            <or>
               <instanceof
                     value="gama.ui.navigator.contents.WrappedSyntacticContent">
               </instanceof>
               <instanceof
                     value="gama.ui.navigator.contents.WrappedFile">
               </instanceof>
           </or>
            </parentExpression>
         </commonSorter>
         <commonSorter
               class="gama.ui.navigator.support.FileFolderSorter">
            <parentExpression>
               <instanceof
                     value="gama.ui.navigator.contents.WrappedContainer">
               </instanceof>
            </parentExpression>
         </commonSorter>
         <dropAssistant
               class="gama.ui.navigator.support.NavigatorDropAssistant"
               id="gama.core.application.dropAssistant1">
            <possibleDropTargets>
               <instanceof
                     value="java.lang.Object">
               </instanceof>
            </possibleDropTargets>
         </dropAssistant>
         <dropAssistant
               class="gama.ui.navigator.support.NavigatorResourceDropAssistant"
               id="gama.ui.navigator.dropAssistant2">
            <possibleDropTargets></possibleDropTargets>
         </dropAssistant>
      </navigatorContent>
      <commonFilter
            activeByDefault="true"
            class="gama.ui.navigator.support.NavigatorFilter"
            id="gama.core.application.shapefile.filter"
            name="Filters Shapefile support files"
            visibleInUI="true">
      </commonFilter>
      <actionProvider
            class="gama.ui.navigator.actions.EditActionProvider"
            id="edit.actions"
            overrides="org.eclipse.ui.navigator.resources.actions.EditActions"
            priority="highest">
         <enablement>
            <with
                  variable="selection">
               <iterate
                     ifEmpty="false"
                     operator="and">
                  <or>
                     <adapt
                           type="org.eclipse.core.resources.IResource">
                     </adapt>
                     <instanceof
                           value="gama.ui.navigator.contents.VirtualContent">
                     </instanceof>
                  </or>
               </iterate>
            </with>
         </enablement>
      </actionProvider>
      <actionProvider
            class="gama.ui.navigator.actions.OpenCloseActionProvider"
            id="openclose.actions"
            priority="highest">
         <enablement>
            <with
                  variable="selection">
               <iterate
                     ifEmpty="false"
                     operator="and">
                  <adapt
                        type="org.eclipse.core.resources.IProject">
                  </adapt>
               </iterate>
            </with>
         </enablement>
      </actionProvider>
      <actionProvider
            class="gama.ui.navigator.actions.RefreshActionProvider"
            id="refresh.actions">
         <enablement>
            <with
                  variable="selection">
               <iterate
                     ifEmpty="true"
                     operator="and">
                  <or>
                     <instanceof
                           value="gama.ui.navigator.contents.TopLevelFolder">
                     </instanceof>
                     <adapt
                           type="org.eclipse.core.resources.IResource">
                     </adapt>
                  </or>
               </iterate>
            </with>
         </enablement>
      </actionProvider>
      <actionProvider
            class="gama.ui.navigator.actions.RefactorActionProvider"
            id="refactor.actions"
            overrides="org.eclipse.ui.internal.navigator.resources.actions.RefactorActionProvider"
            priority="highest">
         <enablement>
            <with
                  variable="selection">
               <iterate
                     ifEmpty="false"
                     operator="and">
                  <adapt
                        type="org.eclipse.core.resources.IResource">
                  </adapt>
               </iterate>
            </with>
         </enablement>
      </actionProvider>
      <actionProvider
            class="gama.ui.navigator.actions.OpenActionProvider"
            id="open.actions">
         <enablement>
            <with
                  variable="selection">
               <iterate
                     ifEmpty="false"
                     operator="and">
                  <instanceof
                        value="gama.ui.navigator.contents.WrappedFile">
                  </instanceof>
               </iterate>
            </with>
         </enablement>
      </actionProvider>
      <actionProvider
            class="gama.ui.navigator.actions.GamlActionProvider"
            id="gaml.actions"
            priority="highest">
             <enablement>
            <with
                  variable="selection">
               <iterate
                     ifEmpty="false"
                     operator="and">
                  <instanceof
                        value="gama.ui.navigator.contents.WrappedSyntacticContent">
                  </instanceof>
               </iterate>
            </with>
         </enablement>
      </actionProvider>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            allowMultiple="false"
            category="gama.gui.category.gama"
            class="gama.ui.navigator.GamaNavigator"
            icon="platform:/plugin/gama.ui.base/icons/view.navigator2.png"
            id="gama.gui.view.GamaNavigator"
            name="Models"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.newWizards">
      <category
            id="gama.gui.wizards.GAMA"
            name="GAMA">
      </category>
      <wizard
            category="gama.gui.wizards.GAMA"
            class="gama.ui.navigator.wizards.NewFileWizard"
            hasPages="true"
            icon="platform:/plugin/gama.ui.base/icons/navigator/new.model2.png"
            id="gama.gui.wizards.NewFileWizard"
            name="Model file"
            project="false">
      </wizard>
       <wizard
             category="gama.gui.wizards.GAMA"
             class="gama.ui.navigator.wizards.NewExperimentWizard"
             hasPages="true"
             icon="platform:/plugin/gama.ui.base/icons/navigator/new.model2.png"
             id="gama.gui.wizards.NewExperimentWizard"
             name="Experiment file"
             project="false">
      </wizard>
      <wizard
            category="gama.gui.wizards.GAMA"
            class="gama.ui.navigator.wizards.NewProjectWizard"
            hasPages="true"
            icon="platform:/plugin/gama.ui.base/icons/navigator/new.project2.png"
            id="gama.gui.wizards.newProjectWizard"
            name="Gama Project"
            project="true">
      </wizard>
      <wizard
            category="gama.gui.wizards.GAMA"
            class="gama.ui.navigator.wizards.NewTestExperimentWizard"
            hasPages="true"
            icon="platform:/plugin/gama.ui.base/icons/navigator/new.model2.png"
            id="gama.gui.wizards.NewTestExperimentWizard"
            name="Test experiment file"
            project="false">
      </wizard>
   </extension>
   <extension
         point="org.eclipse.core.contenttype.contentTypes">
      <content-type
            file-extensions="shp,SHP"
            id="gama.gui.shapefile.type"
            name="Shapefile"
            priority="high">
      </content-type>
      <content-type
            default-charset="UTF-8"
            file-extensions="qml,QML,cpg,CPG,prj,shx,dbf,xml,sbn,sbx,qix,qpj,fix,PRJ,SHX,DBF,XML,SBN,SBX,QIX,QPJ,FIX"
            id="gama.gui.shapefile.support.type"
            name="Shapefile support"
            priority="high">
         <property
               name="gama.support.target">
         </property>
      </content-type>
      <content-type
            file-extensions="PNG,png,GIF,gif,JPEG,jpeg,JPG,jpg,BMP,bmp,ICO,ico,TIFF,tiff,TIF,tif,ASC,asc,PGM,pgm"
            id="gama.gui.images.type"
            name="Image"
            priority="high">
      </content-type>
      <content-type
            base-type="org.eclipse.core.runtime.text"
            default-charset="UTF-8"
            file-extensions="CSV,csv,TSV,tsv"
            id="gama.gui.csv.type"
            name="CSV"
            priority="high">
      </content-type>
      <content-type
            base-type="org.eclipse.core.runtime.text"
            default-charset="UTF-8"
            file-extensions="gaml,experiment"
            id="gama.gui.gaml.type"
            name="GAML"
            priority="high">
      </content-type>
      <content-type
            base-type="org.eclipse.core.runtime.text"
            file-extensions="html, HTML, htm, HTM, svg, SVG"
            id="gama.gui.html.type"
            name="HTML (in GAMA)"
            priority="normal">
      </content-type>
      <content-type
            file-extensions="osm,OSM,pbf,PBF,bz2,BZ2,gz,GZ"
            id="gama.gui.osm.type"
            name="OSM"
            priority="high">
      </content-type>
   </extension>
   <extension
         point="org.eclipse.ui.services">
      <serviceFactory
            factoryClass="gama.ui.navigator.commands.RefreshServiceFactory">
         <service
               serviceClass="gama.ui.base.interfaces.IRefreshHandler">
         </service>
      </serviceFactory>
   </extension>
   <extension
point="org.eclipse.core.expressions.propertyTesters">
<propertyTester
class="gama.ui.navigator.support.ClipboardTester"
id="gama.core.clipboard.tester"
namespace="gama.core"
properties="cbready"
type="java.lang.Object">
</propertyTester>
</extension>
    <extension
          point="org.eclipse.ui.importWizards">
       <wizard
             category="gama.gui.wizards.GAMA"
             class="gama.ui.navigator.wizards.ImportProjectWizard"
             icon="platform:/plugin/gama.ui.base/icons/navigator/navigator.import.project2.png"
             id="gama.ui.base.import.ExternalProject"
             name="GAMA Project">
       </wizard>
       <wizard
             category="gama.gui.wizards.GAMA"
             class="gama.ui.navigator.wizards.FileImportWizard"
             icon="platform:/plugin/gama.ui.base/icons/navigator/navigator.import.disk2.png"
             id="gama.ui.base.import.ExternalFile"
             name="External files from disk">
       </wizard>
       <wizard
             category="gama.gui.wizards.GAMA"
             class="gama.ui.navigator.wizards.ZipImportWizard"
             icon="platform:/plugin/gama.ui.base/icons/navigator/navigator.import.archive2.png"
             id="gama.ui.base.import.ZipFile"
             name="External files from archive">
       </wizard>
       <category
             id="gama.gui.wizards.GAMA"
             name="GAMA">
       </category>
    </extension>
    <extension
          point="org.eclipse.ui.navigator.linkHelper">
       <linkHelper
             class="gama.ui.navigator.support.NavigatorLinkHelper"
             id="gama.ui.navigator.linkHelper1">
          <editorInputEnablement>
             <instanceof
                   value="org.eclipse.ui.part.FileEditorInput">
             </instanceof>
          </editorInputEnablement>
          <selectionEnablement>
             <or>
                <instanceof
                      value="gama.ui.navigator.contents.WrappedFile">
                </instanceof>
                <instanceof
                      value="gama.ui.navigator.contents.WrappedGamaFile">
                </instanceof>
                <adapt
                      type="org.eclipse.core.resources.IFile">
                </adapt>
             </or>
          </selectionEnablement>
       </linkHelper>
    </extension>
    <extension
          point="org.eclipse.ui.propertyPages">
       <page
             class="gama.ui.navigator.properties.TopLevelFolderPropertyPage"
             id="gama.ui.navigator.properties.samplePropertyPage"
             name="Folder"
             nameFilter="*.*">
          <enabledWhen>
             <instanceof
                   value="gama.ui.navigator.contents.TopLevelFolder">
             </instanceof>
          </enabledWhen>
       </page>
    </extension>
    <extension
          point="org.eclipse.ui.exportWizards">
       <wizard
             category="gama.gui.wizards.GAMA"
             class="gama.ui.navigator.wizards.ExportProjectWizard"
             icon="platform:/plugin/gama.ui.base/icons/navigator/navigator.import.archive2.png"
             id="gama.ui.base.export.ExternalProject"
             name="GAMA Project">
       </wizard>
       <wizard
             category="gama.gui.wizards.GAMA"
             class="gama.ui.navigator.wizards.ZipExportWizard"
             icon="platform:/plugin/gama.ui.base/icons/navigator/navigator.import.archive2.png"
             id="gama.ui.base.export.ZipFile"
             name="External files to archive">
       </wizard>
    </extension>
    <extension
          point="org.eclipse.ui.commands">
       <command
             categoryId="gama.category"
             id="gama.core.navigator.reveal"
             name="Reveal in Workspace">
       </command>
    </extension>
    <extension
          point="org.eclipse.ui.handlers">
       <handler
             class="gama.ui.navigator.commands.RevealInWorkspace"
             commandId="gama.core.navigator.reveal">
          <enabledWhen>
             <with
                   variable="selection">
                <iterate
                      ifEmpty="false"
                      operator="and">
                   <instanceof
                         value="gama.ui.navigator.contents.LinkedFile">
                   </instanceof>
                </iterate>
             </with>
          </enabledWhen>
       </handler>
    </extension>
</plugin>
